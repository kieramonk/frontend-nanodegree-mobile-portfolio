    <script type="text/javascript">// <![CDATA[
    $(function(){
    
      // Replace all social sharing links of a 'Simple' type with more descriptive markup and a button
      $( ".en__shares--simple .en__share--simple" ).each(function( index ) {
        var share = $(this);
        var linkElement = share.children('a');
        var snName = '';
        var snButton = 'https://www.childrenssociety.org.uk/sites/default/files/en-assets/';
    
    
         // Need this to remove the EN width/heights
        $('.en__shares--simple').addClass('social-replace');
    
        if($(share).hasClass('en__share--twitter')){
          snName = 'Twitter';
          snButton += 'square-tweet-button.jpg';
        }else if($(share).hasClass('en__share--facebook')){
          snName = 'Facebook';
          snButton += 'square-facebook-share-button.jpg';
        }else if($(share).hasClass('en__share--gplus')){
          snName = 'Google+';
          snButton += 'square-google-share-button.png';
        }
    
        var imageElement = $('<img></img>').attr('src', snButton).attr('alt','');
        linkElement
          .empty()
          .append(imageElement)
          .wrap('<div class="inner"></div>')
          .before('<h3>Share this</h3><p>Click the button below to share the link on ' + snName + '</p>');
      });
    
    });

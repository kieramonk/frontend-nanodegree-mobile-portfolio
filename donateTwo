    <script type="text/javascript" async>
      $(document).ready(function() {
        
         /**
          *
          * Bind the Mega Menu functionality to the global TCS object
          *
          */
          MobileMenu = {
            maxScroll       : 0,
            moving          : false,
            open            : false,
            touchEnd        : 0,
            touchLastClientY: 0,
            touchMoveScroll : 0,
        
           /**
            *
            * A function to initilise the Mobile Menu
            *
            * This function calls a function to
            * initialise all the menu event handlers
            *
            */
            init: function(){
              this.registerEvents();
            },
        
           /**
            *
            * A function to handle the menu display controller click
            *
            * @param jQuery Event e
            *
            */
            controlClicked: function(e){
              e.preventDefault();
        
              if( !this.open ){ this.menuShow(); }
              else{ this.menuHide(); }
            },
        
           /**
            *
            * A function to handle a menu anchor click event
            *
            * This function is used to disable the click event whilst the
            * user is scrolling through the menu
            *
            * @param jQuery Event e
            *
            */
            linkHandler: function(e){
              if(this.moving){ e.preventDefault(); }
            },
        
           /**
            *
            * A wrapper function to hid the mobile menu and remove all touch listeners
            *
            */
            menuHide: function(){
              $('#page-wrapper').removeClass('mobile-nav-showing');
              this.open = false;
        
              $(window).unbind('touchend');
              $(window).unbind('touchmove');
              $(window).unbind('touchstart');
            },
        
           /**
            *
            * A wrapper function to show the mobile menu and register all touch listeners
            *
            */
            menuShow: function(){
              $('#page-wrapper').addClass('mobile-nav-showing');
              this.open = true;
        
              var scrollTop = $(window).scrollTop();
              $('#mobile-menu-wrap').css({'top':scrollTop+'px'});
        
              $(window).on('touchend',  $.proxy(this.touchEndHandler,   this) );
              $(window).on('touchmove', $.proxy(this.touchMoveHandler,  this) );
              $(window).on('touchstart',$.proxy(this.touchStartHandler, this) );
            },
        
           /**
            *
            * A function to register all the event handlers
            *
            */
            registerEvents: function(){
              $(document).on('click', '#mobile-menu-control', $.proxy(this.controlClicked,this));
              $(document).on('click', '#mobile-menu a',       $.proxy(this.linkHandler,this));
        
              $(window).on('load',   $.proxy(this.resizeHandler, this));
              $(window).on('resize', $.proxy(this.resizeHandler, this));
            },
        
           /**
            *
            * A function to handle the window resize an load events
            *
            * This function is used to calculate the maxScroll value
            * based on the current window height
            *
            */
            resizeHandler: function(){
              // Use half the window height so that we can guarantee all
              // menu items will be displayed
              var windowHeight = $(window).height() * 0.5;
              var menuHeight   = $('#mobile-menu').outerHeight();
              this.maxScroll   = (windowHeight - menuHeight);
            },
        
           /**
            *
            * A function to handle the TouchEnd event
            *
            * Assigns the end class to the menu to trigger the CSS transition
            * animation that gives the illusion of scrolling inertia
            *
            */
            touchEndHandler: function(e){
              this.moving = false;
              $('#mobile-menu').addClass('end');
        
              this.touchMoveScroll += this.touchEnd;
        
              if(this.touchMoveScroll > 0){ this.touchMoveScroll = 0; }
              if(this.touchMoveScroll < this.maxScroll){ this.touchMoveScroll = this.maxScroll; }
        
              var css = {
                '-webkit-transform': 'translateY('+this.touchMoveScroll+'px)',
                'transform'        : 'translateY('+this.touchMoveScroll+'px)'
              }
        
              $('#mobile-menu').css(css);
            },
        
           /**
            *
            * A function to handle the TouchMove event
            *
            */
            touchMoveHandler: function(e){
              e.preventDefault();
              $('#mobile-menu').removeClass('end');
        
              // Calculate the scroll size based on the current yPpos
              // asn the previously stored yPos
              var newPos = e.originalEvent.touches[0].clientY;
              var diff   = Math.round((newPos - this.touchLastClientY)*2);
        
              // Remove 10% of the new scroll increment to use when the touch ends
              // This is to give the illusion of scrolling inertia
              this.touchEnd         = Math.round(diff * 0.4);
              this.touchLastClientY = newPos;
              this.touchMoveScroll += (diff - this.touchEnd);
        
              if(this.touchMoveScroll > 0){ this.touchMoveScroll = (0 - this.touchEnd); }
              if(this.touchMoveScroll < this.maxScroll){ this.touchMoveScroll = (this.maxScroll + this.touchEnd); }
        
              var css = {
                '-webkit-transform': 'translateY('+this.touchMoveScroll+'px)',
                'transform'        : 'translateY('+this.touchMoveScroll+'px)'
              }
        
              $('#mobile-menu').css(css);
            },
        
           /**
            *
            * A function to handle the touch start event
            *
            * This is used to store the clientY position when the
            * touch gesture started
            *
            */
            touchStartHandler: function(e){
              this.moving = true;
              this.touchLastClientY = e.originalEvent.touches[0].clientY;
            },
          };
        
          MobileMenu.init();
          function OptInApi() {
            this.introElement = $("div.opt-in-introduction");
            this.emailElement = $("#en__field_supporter_emailAddress");
            this.allQuestions = $(this.questionSelector);
            this.submitButton = $(".en__submit button, .en__ticketSummary button");
        
            this.hideAll();
        
            if (this.emailElement.length){
              this.emailElement.change(this.onEmailChange.bind(this));
        
              if (this.emailElement.val().length) {
                this.onEmailChange();
              }
            }
          };
        
          OptInApi.prototype.questionSelector = "div.opt-in-questions .en__field--question";
          OptInApi.prototype.onEmailChange = function() {
            this.resetForm();
        
            var emailAddress = this.emailElement.val();
            if (emailAddress) {
              clearTimeout(this.optinTimeout);
        
              $.ajax({
                type: 'GET',
                url: "https://www.childrenssociety.org.uk/page/json/" + encodeURIComponent(emailAddress), // Update URL
                success: this.ajaxSuccess.bind(this),
                error: this.ajaxError.bind(this)
              });
        
            }
        
          }
        
          OptInApi.prototype.hideAll = function(){
            this.introElement.hide();
            this.allQuestions.hide();
          };
        
          OptInApi.prototype.resetForm = function(){
            this.introElement.slideUp();
            this.allQuestions.slideUp();
            this.disableSubmit();
            $("input", this.allQuestions).attr('checked', false);
          };
        
          OptInApi.prototype.ajaxSuccess = function(data){
        
            if ($.isEmptyObject(data)){
              data.questions = {};
            }
            // Keep track of unanswered question so we can show them later
            var unansweredQuestions = this.allQuestions;
        
            for (var question in data.questions) {
              if (data.questions.hasOwnProperty(question)) {
                // Work out css class
                var cssName = this.makeCSSName(data.questions[question].name);
                var containingDiv = $(cssName);
                if (containingDiv.length) {
        
                  unansweredQuestions = unansweredQuestions.not(containingDiv);
        
                  // show question if it's undefined or they previously said no
                  var inputElement = $('input', containingDiv);
                  var answer = data.questions[question];
                  if(typeof answer == 'undefined' || ! answer.response){
                    this.showQuestions(containingDiv);
                  }
                  else {
                    inputElement.attr('checked', true);
                  }
                }
              }
            }
            // Show any un-answered questions
            this.showQuestions(unansweredQuestions);
        
            this.setTimeout();
          };
        
          OptInApi.prototype.showQuestions = function(questions){
            if (questions.length){
              this.introElement.slideDown();
              questions.slideDown();
            }
          }
        
          OptInApi.prototype.ajaxError = function() {
            this.enableSubmit();
            this.showQuestions(this.allQuestions);
          };
        
          OptInApi.prototype.makeCSSName = function(rawName) {
            return '.opt-in-questions .en__field--' + rawName.toLowerCase().replace(/[^a-z]/g,'-');
          };
        
          OptInApi.prototype.setTimeout = function(){
            this.optinTimeout = window.setTimeout(this.enableSubmit.bind(this), 1000);
          };
        
          OptInApi.prototype.enableSubmit = function(){
            this.submitButton.attr('disabled', false)
              .css({
                "background-color": "",
                "border-color": ""
              });
          };
        
          OptInApi.prototype.disableSubmit = function(){
            this.submitButton.attr('disabled', true)
              .css({
                "background-color": "#999",
                "border-color": "#999",
                "-webkit-transition": "background-color 0.3s, border-color 0.3s",
                "transition": "background-color 0.3s, border-color 0.3s"
              });
          };
        
          optinAPI = new OptInApi();
        
        function startDateBuilder(element, totalOptions) {
        
        	var dateElement = $(element);
        
        	// do nothing if no matches
        	if (dateElement.length == 0) {
        		return;
        	}
        
        	function convertMonthVal(m){
        		m++;
        		if(m<10){
        			return "0" + m;
        		}
        		return m;
        	};
        
        	// Start with current elements (i.e. empty select this option)
        	var months = ['January', 'February', 'March', 'April', 'May', 'June', 'July', 'August', 'September', 'October','November','December'];
        	var options = [];
        	var now = new Date();
        	now.setDate(now.getDate() + 10);
        	var year = now.getFullYear();
        	var month = now.getMonth();
        	var day = 3;
        	while(options.length < totalOptions){
        		for(var i=0; i<4; i++){
        			day = 3;
        			if(i == 1){
        				day = 10;
        			}
        			else if(i == 2){
        				day = 17;
        			}
        			else if(i == 3){
        				day = 24;
        			}
        			newDate = new Date(year, month, day);
        			if(newDate > now){
        				options.push(newDate);
        			}
        		}
        		if(month == 11){
        			year++;
        			month = 0;
        		}else {
        			month++;
        		}
        	}
        
        	for(var option =0; option < options.length; option++){
        		optionDate = options[option];
        		var valueString = optionDate.getDate() + '/' + optionDate.getMonth() + '/' + optionDate.getFullYear();
        		var textString = optionDate.getDate() + 'th ' + months[optionDate.getMonth()] + ' ' + optionDate.getFullYear();
        		var newOption = $('<option></option>').attr("value", valueString).text(textString);
        		dateElement.append(newOption);
        	}
        }
        
        startDateBuilder('#en__field_supporter_NOT_TAGGED_21',5);
        $('input[name="supporter.NOT_TAGGED_25"]').each(function(){
          var timestamp = $.now();
          var number = Math.floor((Math.random() * 100000000) + 1);
          var rand = number;
          var code = 'EN' + rand;
          $(this).val(code);
        
        });
    
        $('#en__field_supporter_questions_36314').attr('placeholder', 'Start typing a postcode, street or address');
        $('#en__field_supporter_questions_36342').attr('placeholder', 'Start typing a postcode, street or address');
        $('#en__field_supporter_questions_36361').attr('placeholder', 'Start typing a postcode, street or address');
        $('#en__field_supporter_questions_44174').attr('placeholder', 'Start typing a postcode, street or address');
    
        $('.tooltip').each(function(){
          var tooltip = $(this).detach();
          var targetID = tooltip.attr('data-tooltip-for-id');
          var target = $('#' + targetID);
          if (target.length > 0){
            target.wrap("<div class='tooltip-container''></div>").after(tooltip);
            target.parent();
            tooltip.addClass('show-tooltip');
          }
        }).tooltip(
          {
            position: {
              my: "center top+10",
              at: "center bottom+10",
            }
          }
        );
    
      });
    </script>
  </body>
